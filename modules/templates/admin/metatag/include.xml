{* purpose of this template: metatags xml inclusion template in admin area *}
<metatag id="{$item.id}" createdon="{$item.createdDate|dateformat}" updatedon="{$item.updatedDate|dateformat}">
    <id>{$item.id}</id>
    <titleOfEntity><![CDATA[{$item.titleOfEntity}]]></titleOfEntity>
    <title><![CDATA[{$item.title}]]></title>
    <description><![CDATA[{$item.description}]]></description>
    <keywords><![CDATA[{$item.keywords}]]></keywords>
    <theModule><![CDATA[{$item.theModule}]]></theModule>
    <functionOfModule><![CDATA[{$item.functionOfModule}]]></functionOfModule>
    <objectOfModule><![CDATA[{$item.objectOfModule}]]></objectOfModule>
    <nameOfIdentifier><![CDATA[{$item.nameOfIdentifier}]]></nameOfIdentifier>
    <idOfObject>{$item.idOfObject}</idOfObject>
    <extraInfos>{$item.extraInfos}</extraInfos>
</metatag>
